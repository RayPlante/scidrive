<fileprocessors>
	<processor>
		<mimetype>application/fits</mimetype>
		<mimetype>application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</mimetype>
		<mimetype>image/tiff</mimetype>
		<mimetype>text/plain</mimetype>
		<id>fitsprocessor</id>
		<class>edu.jhu.pha.vospace.process.FileToDatabaseProcessor</class>
	</processor>
	<processor>
		<id>docsprocessor</id>
		<mimetype>application/msword</mimetype>
		<mimetype>application/vnd.openxmlformats-officedocument.wordprocessingml.document</mimetype>
		<mimetype>text/plain</mimetype>
		<processor>edu.sdsc.vospace.process.DocumentsProcessor</processor>
	</processor>
	<processor>
		<id>simprocessor</id>
		<mimetype>application/enzoparameterfile</mimetype>
		<processor>edu.sdsc.vospace.process.SimulationProcessor</processor>
	</processor>
</fileprocessors>
